{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "status": {
      "path": "/_v/status/:code",
      "public": true
    }
  },
  "events": {
    "broadcasterSku": {
      "sender": "vtex.broadcaster",
      "keys": ["broadcaster.sku"]
    },
    "broadcasterProduct": {
      "sender": "vtex.broadcaster",
      "keys": ["broadcaster.product"]
    },
    "broadcasterBrand": {
      "sender": "vtex.broadcaster",
      "keys": ["broadcaster.brand"]
    },
    "broadcasterCategory": {
      "sender": "vtex.broadcaster",
      "keys": ["broadcaster.category"]
    }
  }
}
